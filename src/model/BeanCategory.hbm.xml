<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.BeanCategory" table="category" schema="PetManageSystem">
        <id name="cateId">
            <column name="cate_id" sql-type="int(11)"/>
            <generator class="native" />
        </id>
        <property name="cateName">
            <column name="cate_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="cateDetail">
            <column name="cate_detail" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>