<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.BeanOrderDetails" table="order_details" schema="PetManageSystem">
        <composite-id>
            <key-property name="orderId">
                <column name="order_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="prodId">
                <column name="prod_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="num">
            <column name="num" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>e